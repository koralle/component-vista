{
  "name": "@component-vista/admin",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -f .env.development -- vite dev",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "serve": "vite preview",
    "test": "vitest run",
    "deploy": "pnpm run build && wrangler deploy",
    "preview": "pnpm run build && vite preview",
    "cf-typegen": "wrangler types",
    "check": "biome check",
    "check:ci": "biome ci",
    "fix": "biome check --fix",
    "typecheck": "tsgo",
    "auth:generate": "NODE_ENV=development dotenvx run -f .env.development -- pnpm dlx @better-auth/cli generate --config auth.ts --output drizzle/auth/schema.ts",
    "auth-db:generate": "NODE_ENV=development dotenvx run -f .env.development -- drizzle-kit generate --config drizzle.config.auth.ts",
    "auth-db:migrate": "NODE_ENV=development dotenvx run -f .env.development -- pnpm wrangler d1 migrations apply --local AUTH_DB",
    "auth-db:migrate-production": "NODE_ENV=production dotenvx run -f .env.development -- pnpm drizzle-kit migrate -c drizzle.config.auth.ts"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "1.17.0",
    "@component-vista/ui": "workspace:*",
    "@libsql/client": "^0.15.15",
    "@tanstack/react-devtools": "0.8.4",
    "@tanstack/react-router": "1.140.1",
    "@tanstack/react-router-devtools": "1.140.1",
    "@tanstack/react-router-ssr-query": "1.140.1",
    "@tanstack/react-start": "1.140.3",
    "@tanstack/router-plugin": "1.140.1",
    "better-auth": "^1.4.5",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.7",
    "kysely": "^0.28.8",
    "kysely-d1": "^0.4.0",
    "lucide-react": "0.544.0",
    "react": "catalog:",
    "react-dom": "catalog:",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@dotenvx/dotenvx": "catalog:",
    "@tanstack/devtools-vite": "0.3.11",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@typescript/native-preview": "catalog:",
    "@vitejs/plugin-react": "5.0.4",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.21.0",
    "typescript": "catalog:",
    "vite": "7.1.11",
    "vite-bundle-analyzer": "^1.2.3",
    "web-vitals": "5.1.0",
    "wrangler": "4.53.0"
  }
}
